{
	"name": "@eyelet/ui",
	"version": "0.0.3",
	"description": "窄门餐眼组件库demo",
	"main": "lib/varlet.cjs.js",
	"module": "es/index.mjs",
	"typings": "types/index.d.ts",
	"web-types": "highlight/web-types.en-US.json",
	"keywords": [
		"Vue",
		"UI"
	],
	"license": "MIT",
	"sideEffects": [
		"es/**/style/*",
		"lib/**/style/*",
		"es/style.mjs",
		"lib/style.js",
		"*.css"
	],
	"scripts": {
		"dev": "varlet-cli dev",
		"build": "varlet-cli build",
		"preview": "varlet-cli preview",
		"compile": "varlet-cli compile",
		"lint": "varlet-cli lint",
		"changelog": "varlet-cli changelog",
		"release": "pnpm compile && varlet-cli release ",
		"test": "varlet-cli jest",
		"test:watch": "varlet-cli jest -w",
		"test:watchAll": "varlet-cli jest -wa",
		"create:comp": "varlet-cli create"
	},
	"peerDependencies": {
		"vue": "^3.2.0"
	},
	"dependencies": {
		"@eyelet/icons": "workspace:^1.0.0"
	},
	"devDependencies": {
		"@types/node": "^18.14.6",
		"@varlet/cli": "^2.9.0",
		"@varlet/icons": "^2.8.4",
		"@varlet/shared": "^2.9.0",
		"@varlet/touch-emulator": "^2.8.4",
		"@vue/test-utils": "^2.0.2",
		"clipboard": "^2.0.6",
		"esbuild-wasm": "^0.17.10",
		"eslint": "^7.30.0",
		"lint-staged": "^10.5.0",
		"live-server": "^1.2.1",
		"lodash-es": "^4.17.21",
		"postcss-px-to-viewport": "^1.1.1",
		"prettier": "^2.3.2",
		"simple-git-hooks": "^2.8.0",
		"stylelint": "15.2.0",
		"typescript": "^4.4.4",
		"vue": "3.2.25",
		"vue-eslint-parser": "^9.1.0",
		"vue-router": "4.0.12"
	},
	"lint-staged": {
		"*.{ts,tsx,js,vue,less}": "prettier --write",
		"*.{ts,tsx,js,vue}": "eslint --fix",
		"*.{vue,css,less}": "stylelint --fix"
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm exec lint-staged --allow-empty --concurrent false",
		"commit-msg": "pnpm exec varlet-cli commit-lint $1"
	},
	"browserslist": [
		"Chrome >= 54",
		"iOS >= 10"
	],
	"packageManager": "pnpm@7.9.0",
	"files": [
		"es",
		"lib",
		"umd",
		"highlight",
		"types"
	],
	"publishConfig": {
		"access": "public"
	}
}
