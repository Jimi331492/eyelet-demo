{
  "$schema": "https://raw.githubusercontent.com/JetBrains/web-types/master/schema/web-types.json",
  "framework": "vue",
  "version": "2.8.6",
  "name": "VARLET",
  "contributions": {
    "html": {
      "tags": [
        {
          "name": "zm-button",
          "attributes": [
            {
              "name": "color",
              "description": "按钮颜色",
              "default": "default",
              "value": {
                "type": "string",
                "kind": "expression"
              }
            }
          ],
          "events": [
            {
              "name": "click",
              "description": "点击按钮时触发"
            }
          ],
          "slots": [
            {
              "name": "default",
              "description": "按钮内容"
            }
          ]
        },
        {
          "name": "zm-dialog",
          "attributes": [
            {
              "name": "mode",
              "description": "弹窗位置",
              "default": "'center'",
              "value": {
                "type": "`center` | `bottom`",
                "kind": "expression"
              }
            },
            {
              "name": "classname",
              "description": "同一般 ReactNode 节点的 classname (挂在弹窗的 body-content 上)",
              "default": "''",
              "value": {
                "type": "string",
                "kind": "expression"
              }
            },
            {
              "name": "visible",
              "description": "控制弹窗的显示与隐藏",
              "default": "false",
              "value": {
                "type": "boolean",
                "kind": "expression"
              }
            },
            {
              "name": "mask",
              "description": "是否展示遮罩",
              "default": "true",
              "value": {
                "type": "boolean",
                "kind": "expression"
              }
            },
            {
              "name": "maskClosable",
              "description": "点击遮罩是否能关闭",
              "default": "false",
              "value": {
                "type": "boolean",
                "kind": "expression"
              }
            },
            {
              "name": "closable",
              "description": "是否展示关闭按钮",
              "default": "true",
              "value": {
                "type": "boolean",
                "kind": "expression"
              }
            },
            {
              "name": "noPadding",
              "description": "取消弹窗(body-content)默认的 padding 除了靠屏幕的一边(可以通过 classname 设置)",
              "default": "false",
              "value": {
                "type": "boolean",
                "kind": "expression"
              }
            }
          ],
          "events": [
            {
              "name": "close",
              "description": "点击遮罩或者关闭按钮触发"
            }
          ],
          "slots": [
            {
              "name": "default",
              "description": "按钮内容"
            }
          ]
        }
      ],
      "types-syntax": "typescript"
    }
  }
}